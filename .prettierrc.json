{
  "$schema": "https://json.schemastore.org/prettierrc",
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "experimentalTernaries": true,
  "singleAttributePerLine": true,
  "jsxSingleQuote": true,
  "bracketSameLine": false,
  "bracketSpacing": true,
  "quoteProps": "as-needed",
  "trailingComma": "es5",
  "arrowParens": "always"
}
